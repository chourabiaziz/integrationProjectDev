{{ form_start(form) }}


<div class="form-group row">
	<label class="col-sm-2 col-form-label">{{ form_label(form.couverture) }}
	</label>
	<div class="col-sm-10">
		{{ form_widget(form.couverture, { 'attr': {'class': 'form-control form-control-capitalize'} }) }}
	</div>
</div>

<div class="form-group row">
	<label class="col-sm-2 col-form-label">{{ form_label(form.dateDebut) }}
	</label>
	<div class="col-sm-10">
		{{ form_widget(form.dateDebut, { 'attr': {'class': 'form-control form-control-capitalize'} }) }}
	</div>
</div>


<button class="btn btn-primary">{{ button_label|default('Envoyer') }}</button>

{{ form_end(form) }}
