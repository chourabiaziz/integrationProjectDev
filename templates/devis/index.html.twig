{% extends 'base.html.twig' %}

{% block title %}Devis index
{% endblock %}

{% block body %}
	<h1>Devis index</h1>

	<table class="table">
		<thead>
			<tr>
				<th>Id</th>
				<th>Valuer</th>
				<th>Puissance</th>
				<th>type assurance</th>
				<th>carburant</th>
				<th>utilisation</th>
				<th>total</th>

				<th>actions</th>
			</tr>
		</thead>
		<tbody>
			{% for devi in devis %}
				<tr>
					<td>{{ devi.id }}</td>
					<td>{{ devi.valeur}}</td>
					<td>{{ devi.puissance }}</td>
					<td>{{ devi.typeassurance }}</td>
					<td>{{ devi.carburant }}</td>
					<td>{{ devi.utilisation }}</td>
					<td>{{ devi.total }}
						Dt</td>


					<td>
						<a href="{{ path('app_devis_show', {'id': devi.id}) }}">show</a>
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="12">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

{% endblock %}
